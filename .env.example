# Telegram
TG_BOT_TOKEN=
TG_BOT_NAME=MaschaBot

# LLM for Java bot
OPENAI_API_KEY=
CHATGPT_API_URL=https://openrouter.ai/api
CHATGPT_MODEL=x-ai/grok-4.1-fast

# Optional voice/translate
VOICE_MODE=false
VOICE_PROVIDER=elevenlabs
VOICE_MAX_CHARS=450
VOICE_DAILY_LIMIT=20
VOICE_MIN_INTERVAL_SEC=20
VOICE_SUMMARY_ON_LONG=true
ELEVENLABS_API_KEY=
ELEVENLABS_VOICE_ID=
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
YANDEX_TRANSLATE_API_KEY=
YANDEX_SPEECH_API_KEY=

# Python yt-video-analyzer
OPENROUTER_API_KEY=
VIDEO_ANALYZER_LLM_MODEL=openai/gpt-4o-mini
VIDEO_ANALYZER_LLM_TEMPERATURE=0.35
VIDEO_ANALYZER_URL=http://yt-video-analyzer:8000/analyze

# yt-dlp behavior in analyzer
HOST_YTDLP_COOKIES_FILE=/opt/masha/cookies/youtube-cookies.txt
YTDLP_COOKIES_PATH=/app/cookies.txt
YTDLP_MANUAL_MODE=true
YTDLP_INCLUDE_REGULAR_SUBS=false
YTDLP_FALLBACK_REGULAR_ON_EMPTY=true
YTDLP_FALLBACK_LANGS_ON_EMPTY=true
YTDLP_SUB_LANGS=ru,ru-orig
YTDLP_SUB_LANGS_FALLBACK=en,en-orig
YTDLP_EXTRACTOR_ARGS=
YTDLP_KEEP_TMP=false
ANALYZE_CACHE_TTL_SEC=900
YTDLP_DEBUG=false
VIDEO_ANALYZER_LOG_LEVEL=INFO
